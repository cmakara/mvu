{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-stage-1",
      {
        "decoratorsLegacy": true
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs"
      ]
    }
  }
}